{
  "manifest_version": 2,
  "name": "Telegra.ph media downloader",
  "description": "Super simple, fast and clean image, video and audio downloader from telegra.ph and graph.org",
  "version": "2.1.0",
  "icons": {
    "16": "images/logo/16.png",
    "48": "images/logo/48.png",
    "128": "images/logo/128.png"
  },
  "page_action": {
    "default_icon": {
      "16": "images/logo/16.png",
      "48": "images/logo/48.png",
      "128": "images/logo/128.png"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["activeTab", "contextMenus", "scripting", "downloads"]
}
